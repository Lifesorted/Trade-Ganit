import net.serenitybdd.screenplay.actions.Click;

/**
 * @author Shaile908568
 *
 */
public class FillAgencyContact extends UIInteractions implements Task{

	 public FillAgencyContact() {
	      
	    }

	 public static FillAgencyContact fromUnderlineDetails() {

	        return new FillAgencyContact();

	    }
	    
	@Override
	public <T extends Actor> void performAs(T actor) {
			
		actor.attemptsTo(Click.on(ProducerDirectDepositDetails.DIRECT_DEPOSIT_DETAIL_TAB));
		waitABit(999);
	}

}
